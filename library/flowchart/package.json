  "description": "A powerful approval workflow flowchart editor based on AntV X6",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "cd examples && pnpm install && pnpm dev",
