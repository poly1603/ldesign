{
  "name": "@ldesign/cropper",
  "type": "module",
  "version": "0.1.0",
  "description": "A powerful, framework-agnostic image cropping library with Vue 3, React, Angular support",
  "author": "LDesign Team",
  "license": "MIT",
  "keywords": [
    "cropper",
    "image-cropper",
    "canvas",
    "image-processing",
    "vue3",
    "react",
    "angular",
    "typescript",
    "framework-agnostic",
    "responsive",
    "touch-support"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/esm/vue/index.js",
      "require": "./dist/cjs/vue/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/esm/react/index.js",
      "require": "./dist/cjs/react/index.cjs"
    },
    "./angular": {
      "types": "./dist/angular/index.d.ts",
      "import": "./dist/esm/angular/index.js",
      "require": "./dist/cjs/angular/index.cjs"
    },
    "./style.css": "./dist/index.css"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,umd",
    "build:prod": "pnpm run clean && ldesign-builder build -f esm,cjs,umd --minify",
    "dev": "ldesign-builder watch",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "type-check:test": "tsc --noEmit --project tsconfig.test.json",
    "lint": "eslint . --fix --cache",
    "lint:check": "eslint . --cache",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rimraf esm cjs dist types lib coverage .nyc_output .tsbuildinfo node_modules/.cache",
    "size-check": "size-limit",
    "prepublishOnly": "pnpm run clean && pnpm run build:prod && pnpm run test:run && pnpm run lint:check",
    "build:check": "pnpm run build && node ../../tools/scripts/build/bundle-validator.js",
    "build:analyze": "pnpm run build && node ../../tools/scripts/build/bundle-analyzer.js",
    "build:validate": "pnpm run build && node ../../tools/scripts/build/validate-build.js",
    "build:browser-test": "pnpm run build && node ../../tools/scripts/build/browser-tester.js",
    "docs:dev": "vitepress dev docs --port 3000",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:serve": "vitepress serve docs --port 4173",
    "ci": "pnpm run lint:check && pnpm run type-check && pnpm run test:run",
    "ci:coverage": "pnpm run lint:check && pnpm run type-check && pnpm run test:coverage",
    "release": "pnpm run ci:coverage && pnpm run build:prod && changeset publish",
    "build:dev": "node scripts/build.js --dev",
    "prepare-release": "node scripts/prepare-release.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\""
  },
  "peerDependencies": {
    "@angular/core": "^17.0.0",
    "react": "^18.0.0",
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    }
  },
  "dependencies": {
    "@ldesign/shared": "workspace:*",
    "lucide": "^0.544.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.32.0",
    "jsdom": "^24.0.0",
    "less": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.12",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "50 KB"
    }
  ]
}
