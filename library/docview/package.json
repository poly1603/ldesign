{
  "name": "@ldesign/docview",
  "type": "module",
  "version": "0.1.0",
  "description": "A framework-agnostic document viewer library supporting Word, Excel, and PowerPoint files with Vue 3 integration",
  "author": "ldesign",
  "license": "MIT",
  "keywords": [
    "document-viewer",
    "office-viewer",
    "word-viewer",
    "excel-viewer",
    "powerpoint-viewer",
    "ppt-viewer",
    "docx-viewer",
    "xlsx-viewer",
    "vue3",
    "typescript",
    "framework-agnostic"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./adapt/vue": {
      "types": "./types/adapt/vue/index.d.ts",
      "import": "./esm/adapt/vue/index.js",
      "require": "./cjs/adapt/vue/index.js"
    },
    "./vue": {
      "types": "./types/adapt/vue/index.d.ts",
      "import": "./esm/adapt/vue/index.js",
      "require": "./cjs/adapt/vue/index.js"
    },
    "./esm/adapt/vue/index.css": "./esm/adapt/vue/index.css",
    "./cjs/adapt/vue/index.css": "./cjs/adapt/vue/index.css",
    "./adapt/vue/index.css": "./esm/adapt/vue/index.css",
    "./style.css": "./esm/adapt/vue/index.css"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist",
    "esm",
    "cjs",
    "types"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:rollup": "rollup -c",
    "build:watch": "ldesign-builder build --watch",
    "dev": "rollup -c -w",
    "type-check": "vue-tsc --noEmit --project tsconfig.json",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf esm cjs dist types lib coverage .nyc_output",
    "size-check": "size-limit",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run build:rollup && pnpm run test:run"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "mammoth": "^1.6.0",
    "x-spreadsheet": "^1.1.9",
    "xlsx": "^0.18.5",
    "jszip": "^3.10.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@babel/core": "^7.25.0",
    "@babel/preset-typescript": "^7.24.7",
    "@vue/babel-plugin-jsx": "^1.2.2",
    "rollup-plugin-postcss": "^4.0.2",
    "less": "^4.2.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.32.0",
    "jsdom": "^24.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.12",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "200 KB"
    }
  ],
  "browser": "dist/index.js"
}
