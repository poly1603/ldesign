{
  "name": "@ldesign/launcher",
  "type": "module",
  "version": "1.0.0",
  "description": "基于Vite JavaScript API的前端项目启动器，支持Vue2/Vue3/React/原生JS/Lit等主流前端技术栈",
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/packages/tree/main/packages/launcher#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/packages",
    "directory": "packages/launcher"
  },
  "bugs": {
    "url": "https://github.com/ldesign/packages/issues"
  },
  "keywords": [
    "vite",
    "launcher",
    "frontend",
    "build-tool",
    "vue",
    "react",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ldesign-launcher": "./bin/cli.js"
  },
  "files": [
    "README.md",
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:e2e": "../../node_modules/.bin/playwright test",
    "test:e2e:ui": "../../node_modules/.bin/playwright test --ui",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "vue": "^2.7.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fast-glob": "^3.3.2",
    "jiti": "^1.21.7",
    "picocolors": "^1.0.0",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.55.0",
    "playwright": "^1.40.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.5.0",
    "typescript": "^5.3.0",
    "vitepress": "^1.6.4",
    "vitest": "^1.0.0"
  }
}
