{
  "name": "@ldesign/captcha",
  "version": "1.0.0",
  "description": "功能完整的网页验证码插件库，支持多种验证方式",
  "keywords": [
    "captcha",
    "verification",
    "puzzle",
    "slider",
    "click",
    "security",
    "human-verification",
    "ldesign"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/captcha#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/captcha.git"
  },
  "bugs": {
    "url": "https://github.com/ldesign/captcha/issues"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./vue": {
      "types": "./es/adapters/vue.d.ts",
      "import": "./es/adapters/vue.js",
      "require": "./lib/adapters/vue.js"
    },
    "./react": {
      "types": "./es/adapters/react.d.ts",
      "import": "./es/adapters/react.js",
      "require": "./lib/adapters/react.js"
    },
    "./angular": {
      "types": "./es/adapters/angular.d.ts",
      "import": "./es/adapters/angular.js",
      "require": "./lib/adapters/angular.js"
    },
    "./styles": "./es/styles/index.css"
  },
  "files": [
    "lib",
    "es",
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "pnpm run build:watch",
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "example:dev": "cd examples/basic && pnpm dev",
    "example:build": "cd examples/basic && pnpm build",
    "clean": "rimraf lib es dist coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "lint:fix": "eslint . --ext .ts,.tsx,.vue,.js,.jsx --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "happy-dom": "^12.0.0",
    "less": "^4.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vitepress": "^1.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "vue": ">=3.0.0",
    "react": ">=16.8.0",
    "@angular/core": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
