{
  "name": "@ldesign/icons-lit",
  "version": "1.0.0",
  "description": "Lit Web Component icons for LDesign",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "unpkg": "dist/umd/index.min.js",
  "jsdelivr": "dist/umd/index.min.js",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist es lib && rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist es lib"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "rollup": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldesign/icons.git",
    "directory": "packages/icons-lit"
  },
  "keywords": [
    "lit",
    "web-components",
    "icons",
    "svg",
    "components",
    "ldesign"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ldesign/icons/issues"
  },
  "homepage": "https://github.com/ldesign/icons#readme",
  "umd": {
    "name": "ldesign-icons-lit",
    "global": "LDesignIconsLit",
    "globals": {
      "lit": "Lit"
    }
  }
}
