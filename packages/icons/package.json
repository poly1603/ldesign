{
  "name": "@ldesign/icons",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "LDesign Icons - A comprehensive icon library for Vue, React, and Web Components",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf packages/*/dist packages/*/es packages/*/lib",
    "build:svg": "node scripts/generate-svg.js",
    "build:components": "node scripts/generate-components.js",
    "build:vue": "npm run build --workspace=@ldesign/icons-vue",
    "build:vue2": "npm run build --workspace=@ldesign/icons-vue2",
    "build:react": "npm run build --workspace=@ldesign/icons-react",
    "build:lit": "npm run build --workspace=@ldesign/icons-lit",
    "build": "npm run clean && npm run build:svg && npm run build:components && npm run build:packages",
    "build:packages": "npm run build:vue && npm run build:vue2 && npm run build:react && npm run build:lit",
    "dev": "npm run build:svg && npm run build:components",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx,.vue,.js,.jsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,css,scss,md}\""
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.2",
    "camelcase": "^7.0.1",
    "chalk": "^4.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-vue": "^9.20.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "svgo": "^3.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "yargs": "^17.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldesign/icons.git"
  },
  "keywords": [
    "icons",
    "svg",
    "vue",
    "react",
    "web-components",
    "lit",
    "design-system"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ldesign/icons/issues"
  },
  "homepage": "https://github.com/ldesign/icons#readme"
}
