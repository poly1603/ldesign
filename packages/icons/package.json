{
  "name": "@ldesign/icons",
  "version": "1.0.0",
  "description": "Modern icon system for LDesign - Compatible with TDesign Icons",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "cross-env gulp --require ts-node/register/transpile-only",
    "build:react": "cross-env gulp buildReact --require ts-node/register/transpile-only",
    "build:vue": "cross-env gulp buildVue --require ts-node/register/transpile-only",
    "build:vue-next": "cross-env gulp buildVueNext --require ts-node/register/transpile-only",
    "build:angular": "cross-env gulp buildAngular --require ts-node/register/transpile-only",
    "build:web-components": "cross-env gulp buildWebComponents --require ts-node/register/transpile-only",
    "build:react-native": "cross-env gulp buildReactNative --require ts-node/register/transpile-only",
    "build:flutter": "cross-env gulp buildFlutter --require ts-node/register/transpile-only",
    "build:svg": "cross-env gulp buildSvg --require ts-node/register/transpile-only",
    "build:fast": "cross-env gulp buildFast --require ts-node/register/transpile-only",
    "build:all": "npm run generate",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/node": "^20.0.0",
    "@types/svg-parser": "^2.0.6",
    "@types/svgo": "^3.0.0",
    "camelcase": "^8.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "decamelize": "^6.0.0",
    "del": "^7.1.0",
    "esbuild": "^0.25.9",
    "fs-extra": "^11.3.2",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.9",
    "gulp-iconfont": "^12.0.0",
    "gulp-iconfont-css": "^3.0.0",
    "gulp-rename": "^2.1.0",
    "merge-stream": "^2.0.0",
    "merge2": "^1.4.1",
    "oslllo-svg-fixer": "^5.0.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.2",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-multi-input": "^1.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "svg-parser": "^2.0.4",
    "svgo": "^3.0.0",
    "through2": "^4.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "uppercamelcase": "^3.0.0",
    "vinyl-fs": "^4.0.2",
    "xmldom": "^0.6.0"
  },
  "dependencies": {
    "jsdom": "^24.0.0"
  }
