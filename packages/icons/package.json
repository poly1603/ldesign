{
  "name": "@ldesign/icons",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "description": "LDesign Icons - A comprehensive icon library for Vue, React, and Web Components",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf dist es lib && rimraf packages/icons-vue/dist packages/icons-vue2/dist packages/icons-react/dist packages/icons-lit/dist packages/shared/dist",
    "build:svg": "node scripts/generate-svg.js",
    "build:components": "node scripts/generate-components.js",
    "build:vue": "pnpm --filter=@ldesign/icons-vue build",
    "build:vue2": "pnpm --filter=@ldesign/icons-vue2 build",
    "build:react": "pnpm --filter=@ldesign/icons-react build",
    "build:lit": "pnpm --filter=@ldesign/icons-lit build",
    "build": "pnpm clean && pnpm build:svg && pnpm build:components && pnpm build:packages",
    "build:packages": "pnpm build:vue && pnpm build:vue2 && pnpm build:react && pnpm build:lit",
    "build:all": "node scripts/build-all.js",
    "generate:packages": "node scripts/build-all.js",
    "build:lib": "tsc && node scripts/fix-imports.js",
    "dev": "pnpm build:svg && pnpm build:components",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.vue,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,vue,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "camelcase": "^7.0.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-vue": "^9.20.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "jsdom": "^24.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "svgo": "^3.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitepress": "^1.0.0-rc.40",
    "vitest": "^1.2.0",
    "vue": "^3.4.0",
    "yargs": "^17.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ldesign/icons.git"
  },
  "keywords": [
    "icons",
    "svg",
    "vue",
    "react",
    "web-components",
    "lit",
    "design-system"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ldesign/icons/issues"
  },
  "homepage": "https://github.com/ldesign/icons#readme"
}
