{
  "name": "@ldesign/watermark",
  "type": "module",
  "version": "0.1.0",
  "description": "LDesign watermark package",
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/{{PACKAGE_NAME}}#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/{{PACKAGE_NAME}}"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "keywords": [
    "ldesign",
    "vue3",
    "typescript",
    "watermark"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "types"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rimraf dist es lib types coverage .nyc_output",
    "size-check": "size-limit",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^2.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.0.0",
    "jsdom": "^24.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.12",
    "vitest": "^2.0.0",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "25 KB"
    }
  ]
}
