{
  "name": "@ldesign/table",
  "version": "1.0.0",
  "description": "功能完整的表格插件 - 支持虚拟滚动、固定列、多选、排序等高级功能",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./es/index.css",
      "require": "./dist/index.css"
    },
    "./vue": {
      "import": "./es/adapters/vue.js",
      "require": "./dist/adapters/vue.js",
      "types": "./dist/adapters/vue.d.ts"
    },
    "./react": {
      "import": "./es/adapters/react.js",
      "require": "./dist/adapters/react.js",
      "types": "./dist/adapters/react.d.ts"
    },
    "./angular": {
      "import": "./es/adapters/angular.js",
      "require": "./dist/adapters/angular.js",
      "types": "./dist/adapters/angular.d.ts"
    }
  },
  "files": [
    "dist",
    "es",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "ldesign-builder",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist es coverage"
  },
  "keywords": [
    "table",
    "grid",
    "virtual-scroll",
    "typescript",
    "framework-agnostic",
    "vue",
    "react",
    "angular",
    "ldesign"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/table"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/table#readme",
  "peerDependencies": {
    "vue": ">=3.0.0",
    "react": ">=16.8.0",
    "@angular/core": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    }
  },
  "dependencies": {
    "@ldesign/shared": "workspace:^"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:^",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.0.0",
    "happy-dom": "^18.0.1",
    "less": "^4.4.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.18"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
