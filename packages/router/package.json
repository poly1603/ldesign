{
  "name": "@ldesign/router",
  "version": "1.0.0",
  "description": "A modern, high-performance Vue router library with TypeScript support, independent of vue-router",
  "keywords": [
    "vue",
    "router",
    "navigation",
    "spa",
    "typescript",
    "performance",
    "modern",
    "independent"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/router#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/router"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./components": {
      "types": "./es/components/index.d.ts",
      "import": "./es/components/index.js",
      "require": "./lib/components/index.cjs"
    },
    "./components/*": {
      "types": "./es/components/*.d.ts",
      "import": "./es/components/*.js",
      "require": "./lib/components/*.cjs"
    },
    "./composables": {
      "types": "./es/composables/index.d.ts",
      "import": "./es/composables/index.js",
      "require": "./lib/composables/index.cjs"
    },
    "./composables/*": {
      "types": "./es/composables/*.d.ts",
      "import": "./es/composables/*.js",
      "require": "./lib/composables/*.cjs"
    },
    "./core": {
      "types": "./es/core/index.d.ts",
      "import": "./es/core/index.js",
      "require": "./lib/core/index.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./debug/*": {
      "types": "./es/debug/*.d.ts",
      "import": "./es/debug/*.js",
      "require": "./lib/debug/*.cjs"
    },
    "./device": {
      "types": "./es/device/index.d.ts",
      "import": "./es/device/index.js",
      "require": "./lib/device/index.cjs"
    },
    "./device/*": {
      "types": "./es/device/*.d.ts",
      "import": "./es/device/*.js",
      "require": "./lib/device/*.cjs"
    },
    "./engine": {
      "types": "./es/engine/index.d.ts",
      "import": "./es/engine/index.js",
      "require": "./lib/engine/index.cjs"
    },
    "./engine/*": {
      "types": "./es/engine/*.d.ts",
      "import": "./es/engine/*.js",
      "require": "./lib/engine/*.cjs"
    },
    "./guards": {
      "types": "./es/guards/index.d.ts",
      "import": "./es/guards/index.js",
      "require": "./lib/guards/index.cjs"
    },
    "./guards/*": {
      "types": "./es/guards/*.d.ts",
      "import": "./es/guards/*.js",
      "require": "./lib/guards/*.cjs"
    },
    "./optimization/*": {
      "types": "./es/optimization/*.d.ts",
      "import": "./es/optimization/*.js",
      "require": "./lib/optimization/*.cjs"
    },
    "./plugins": {
      "types": "./es/plugins/index.d.ts",
      "import": "./es/plugins/index.js",
      "require": "./lib/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./es/plugins/*.d.ts",
      "import": "./es/plugins/*.js",
      "require": "./lib/plugins/*.cjs"
    },
    "./security/*": {
      "types": "./es/security/*.d.ts",
      "import": "./es/security/*.js",
      "require": "./lib/security/*.cjs"
    },
    "./smart/*": {
      "types": "./es/smart/*.d.ts",
      "import": "./es/smart/*.js",
      "require": "./lib/smart/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "dev": "rollup -c -w",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:performance": "node scripts/performance-test.js",
    "benchmark": "node scripts/performance-benchmark.js",
    "benchmark:regression": "node scripts/performance-regression.js",
    "benchmark:baseline": "node scripts/performance-regression.js --create-baseline",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rimraf esm cjs dist types lib coverage .nyc_output",
    "size-check": "size-limit",
    "analyze": "rollup -c --bundleConfigAsCjs --environment ANALYZE",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:check": "pnpm run build && node ../../tools/scripts/build/bundle-validator.js",
    "build:analyze": "pnpm run build && node ../../tools/scripts/build/bundle-analyzer.js",
    "build:validate": "pnpm run build && node ../../tools/scripts/build/validate-build.js",
    "build:browser-test": "pnpm run build && node ../../tools/scripts/build/browser-tester.js",
    "validate": "pnpm run type-check && pnpm run test:run && pnpm run build",
    "validate:quick": "pnpm run type-check",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "commit-msg": "commitlint --edit",
    "commit:interactive": "node scripts/commit.js",
    "validate:full": "node scripts/validate.js",
    "validate:e2e": "node scripts/validate.js --e2e",
    "build:dev": "node scripts/build.js --dev"
  },
  "dependencies": {
    "@ldesign/device": "workspace:*"
  },
  "peerDependencies": {
    "@ldesign/device": "^1.0.0",
    "@ldesign/engine": "^1.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "@playwright/test": "^1.54.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.0.0",
    "jsdom": "^24.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.12",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "size-limit": [
    {
      "name": "UMD Bundle",
      "path": "dist/index.js",
      "limit": "45 KB"
    },
    {
      "name": "UMD Bundle (minified)",
      "path": "dist/index.min.js",
      "limit": "20 KB"
    },
    {
      "name": "ES Module",
      "path": "esm/index.js",
      "limit": "40 KB",
      "import": "{ createRouter }"
    },
    {
      "name": "CommonJS",
      "path": "cjs/index.js",
      "limit": "40 KB"
    }
  ],
  "peerDependenciesMeta": {
    "@ldesign/device": {
      "optional": false
    },
    "@ldesign/engine": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}