{
  "name": "@ldesign/example-multi-module-typescript",
  "version": "1.0.0",
  "description": "多模块 TypeScript 库示例",
  "main": "cjs/index.cjs",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./cjs/index.cjs",
      "types": "./es/index.d.ts"
    },
    "./utils": {
      "import": "./es/utils/index.js",
      "require": "./cjs/utils/index.cjs",
      "types": "./es/utils/index.d.ts"
    },
    "./components": {
      "import": "./es/components/index.js",
      "require": "./cjs/components/index.cjs",
      "types": "./es/components/index.d.ts"
    },
    "./types": {
      "import": "./es/types/index.js",
      "require": "./cjs/types/index.cjs",
      "types": "./es/types/index.d.ts"
    }
  },
  "files": [
    "es",
    "cjs"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "clean": "rimraf es cjs dist types"
  },
  "keywords": [
    "typescript",
    "library",
    "multi-module",
    "example"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "rimraf": "^5.0.0"
  }
}
