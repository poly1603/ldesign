{
  "name": "@ldesign/datepicker",
  "version": "1.0.0",
  "description": "跨平台日期选择器组件库，支持 PC、平板、手机三端响应式适配，框架无关实现",
  "keywords": [
    "datepicker",
    "calendar",
    "date",
    "time",
    "picker",
    "responsive",
    "cross-platform",
    "framework-agnostic",
    "typescript",
    "ldesign"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js",
      "types": "./es/index.d.ts"
    },
    "./styles": {
      "import": "./es/styles/index.css",
      "require": "./lib/styles/index.css"
    }
  },
  "files": [
    "lib",
    "es",
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "ldesign-launcher dev",
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "clean": "rimraf lib es dist coverage",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "dependencies": {},
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/launcher": "workspace:*",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "jsdom": "^23.0.0",
    "less": "^4.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitepress": "^1.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/datepicker"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "homepage": "https://ldesign.dev/datepicker"
}
