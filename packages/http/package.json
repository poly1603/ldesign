{
  "name": "@ldesign/http",
  "type": "module",
  "version": "0.1.0",
  "description": "一个功能强大的通用 HTTP 请求库，支持多种适配器、拦截器、缓存、重试等高级功能，完美集成 Vue 3",
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/http#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/http.git"
  },
  "bugs": {
    "url": "https://github.com/ldesign/http/issues"
  },
  "keywords": [
    "http",
    "request",
    "client",
    "fetch",
    "axios",
    "alova",
    "vue3",
    "typescript",
    "interceptor",
    "cache",
    "retry",
    "adapter"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./es/index.js",
      "require": "./dist/index.cjs"
    },
    "./vue": {
      "types": "./types/vue.d.ts",
      "import": "./es/vue.js",
      "require": "./dist/vue.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "es",
    "types"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "clean": "rimraf dist es lib types coverage .nyc_output",
    "size-check": "size-limit",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run",
    "deploy": "tsx ../../tools/deploy/package-deployer.ts http",
    "deploy:beta": "tsx ../../tools/deploy/package-deployer.ts http --tag beta",
    "deploy:alpha": "tsx ../../tools/deploy/package-deployer.ts http --tag alpha",
    "deploy:dry-run": "tsx ../../tools/deploy/package-deployer.ts http --dry-run"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@playwright/test": "^1.40.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "@vue/test-utils": "^2.4.0",
    "alova": "^3.0.0",
    "axios": "^1.6.0",
    "eslint": "^9.0.0",
    "jsdom": "^24.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.9.0",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.0",
    "vitepress": "^1.6.3",
    "vitest": "^2.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  }
}
