{
  "name": "@ldesign/map",
  "version": "1.0.0",
  "description": "基于 OpenLayers 的通用地图插件，支持多种地图类型、框架兼容性和丰富的地图功能",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vue": {
      "types": "./dist/types/adapters/vue/index.d.ts",
      "import": "./dist/vue.js",
      "require": "./dist/vue.cjs"
    },
    "./react": {
      "types": "./dist/types/adapters/react/index.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    },
    "./vanilla": {
      "types": "./dist/types/adapters/vanilla/index.d.ts",
      "import": "./dist/vanilla.js",
      "require": "./dist/vanilla.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:lib",
    "build:lib": "vite build && tsc --project tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:performance": "vitest run tests/performance-simple.test.ts",
    "test:integration": "vitest run src/__tests__/integration/",
    "lint": "eslint src --ext .ts,.tsx,.vue",
    "lint:fix": "eslint src --ext .ts,.tsx,.vue --fix",
    "type-check": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build && npm run test",
    "release": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "keywords": [
    "map",
    "openlayers",
    "vue",
    "react",
    "typescript",
    "gis",
    "geospatial",
    "routing",
    "geofence",
    "heatmap",
    "markers",
    "layers",
    "search",
    "measurement",
    "drawing",
    "ldesign"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "dependencies": {
    "@turf/along": "^7.2.0",
    "@turf/bbox": "^7.2.0",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "@turf/buffer": "^7.2.0",
    "@turf/center": "^7.2.0",
    "@turf/distance": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@turf/length": "^7.2.0",
    "@turf/turf": "^7.2.0",
    "ol": "^8.2.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/launcher": "workspace:*",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^25.0.0",
    "less": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitepress": "^1.3.4",
    "vitest": "^2.0.5",
    "vue": "^3.4.38"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "typescript": ">=5.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/map"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "homepage": "https://ldesign.dev/map"
}
