{
  "name": "@ldesign/app",
  "type": "module",
  "version": "0.1.0",
  "description": "ðŸš€ LDesign å®Œæ•´åº”ç”¨æ¨¡æ¿ - åŸºäºŽ Vue 3 + TypeScript + LDesign ç”Ÿæ€ç³»ç»Ÿçš„çŽ°ä»£åŒ–åº”ç”¨å¼€å‘æ¨¡æ¿",
  "author": "LDesign Team",
  "license": "MIT",
  "keywords": [
    "ldesign",
    "vue3",
    "typescript",
    "tsx",
    "vite",
    "rollup",
    "template",
    "starter",
    "boilerplate",
    "app-template",
    "modern",
    "enterprise"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "cjs",
    "dist",
    "esm",
    "types"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --force",
    "dev:built": "vite --config vite.config.built.ts --force",
    "dev:source": "cross-env VITE_DEV_MODE=source vite --config vite.config.source.ts --force",
    "dev:source-simple": "vite index.simple.html --config vite.config.simple-source.ts --force --port 3003",
    "dev:compare": "concurrently \"pnpm run dev:built\" \"pnpm run dev:no-check\"",
    "dev:no-check": "vite --force --port 3001",
    "dev:built:no-check": "vite --config vite.config.built.ts --force --port 3001",
    "dev:source:no-check": "cross-env VITE_DEV_MODE=source vite --config vite.config.source.ts --force --port 3002",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" rollup -c",
    "build:watch": "rollup -c -w",
    "build:vite": "vue-tsc --noEmit && vite build",
    "build:vite:watch": "vite build --watch",
    "build:vite:built": "vite build --config vite.config.built.ts",
    "build:vite:source": "cross-env VITE_DEV_MODE=source vite build --config vite.config.source.ts",
    "preview": "vite preview",
    "preview:built": "vite preview --config vite.config.built.ts",
    "preview:source": "vite preview --config vite.config.source.ts",
    "env": "tsx scripts/dev-env-switcher.ts",
    "env:list": "tsx scripts/dev-env-switcher.ts list",
    "env:built": "tsx scripts/dev-env-switcher.ts start built",
    "env:source": "tsx scripts/dev-env-switcher.ts start source",
    "env:compare": "tsx scripts/dev-env-switcher.ts compare",
    "test:modes": "tsx scripts/test-both-modes.ts",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "clean": "rimraf esm cjs dist types lib coverage .nyc_output",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  },
  "dependencies": {
    "@ldesign/cache": "workspace:*",
    "@ldesign/color": "workspace:*",
    "@ldesign/crypto": "workspace:*",
    "@ldesign/device": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "@ldesign/http": "workspace:*",
    "@ldesign/i18n": "workspace:*",
    "@ldesign/router": "workspace:^",
    "@ldesign/size": "workspace:*",
    "@ldesign/store": "workspace:*",
    "@ldesign/template": "workspace:*",
    "alova": "^3.3.4",
    "pinia": "^2.1.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@rollup/plugin-commonjs": "^26.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^12.0.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/babel-plugin-jsx": "^1.4.0",
    "@vue/test-utils": "^2.4.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.0.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "monaco-editor": "^0.52.0",
    "prismjs": "^1.29.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-esbuild": "^6.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.12",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "browser": "dist/index.min.js"
}
