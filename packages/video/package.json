{
  "name": "@ldesign/video",
  "version": "0.1.0",
  "description": "A universal video player component library with cross-device compatibility, plugin system, theme system, and danmaku support",
  "keywords": [
    "video",
    "player",
    "video-player",
    "cross-device",
    "cross-framework",
    "plugin-system",
    "theme-system",
    "danmaku",
    "subtitle",
    "typescript",
    "responsive",
    "mobile",
    "desktop"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./core": {
      "types": "./es/core/index.d.ts",
      "import": "./es/core/index.js",
      "require": "./lib/core/index.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./frameworks": {
      "types": "./es/frameworks/index.d.ts",
      "import": "./es/frameworks/index.js",
      "require": "./lib/frameworks/index.cjs"
    },
    "./frameworks/*": {
      "types": "./es/frameworks/*.d.ts",
      "import": "./es/frameworks/*.js",
      "require": "./lib/frameworks/*.cjs"
    },
    "./plugins": {
      "types": "./es/plugins/index.d.ts",
      "import": "./es/plugins/index.js",
      "require": "./lib/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./es/plugins/*.d.ts",
      "import": "./es/plugins/*.js",
      "require": "./lib/plugins/*.cjs"
    },
    "./themes": {
      "types": "./es/themes/index.d.ts",
      "import": "./es/themes/index.js",
      "require": "./lib/themes/index.cjs"
    },
    "./themes/*": {
      "types": "./es/themes/*.d.ts",
      "import": "./es/themes/*.js",
      "require": "./lib/themes/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    },
    "./style.css": "./src/index.css"
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs",
    "build:prod": "pnpm run clean && ldesign-builder build -f esm,cjs --minify",
    "dev": "ldesign-builder watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist es cjs coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "example:dev": "cd examples/vanilla && launcher dev",
    "example:build": "cd examples/vanilla && launcher build",
    "example:preview": "cd examples/vanilla && launcher preview",
    "test:run": "vitest run"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": ">=3.0.0",
    "react": ">=16.8.0",
    "@angular/core": ">=12.0.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/launcher": "workspace:*",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.55.0",
    "less": "^4.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0",
    "vitepress": "^1.6.4",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "style": "dist/index.css",
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.0.0"
}