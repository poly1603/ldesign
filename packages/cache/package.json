{
  "name": "@ldesign/cache",
  "version": "0.1.0",
  "description": "LDesign cache package",
  "keywords": [
    "ldesign",
    "vue3",
    "typescript",
    "cache"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/cache#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/cache"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./engine/*": {
      "types": "./es/engine/*.d.ts",
      "import": "./es/engine/*.js",
      "require": "./lib/engine/*.cjs"
    },
    "./engines/*": {
      "types": "./es/engines/*.d.ts",
      "import": "./es/engines/*.js",
      "require": "./lib/engines/*.cjs"
    },
    "./security/*": {
      "types": "./es/security/*.d.ts",
      "import": "./es/security/*.js",
      "require": "./lib/security/*.cjs"
    },
    "./strategies/*": {
      "types": "./es/strategies/*.d.ts",
      "import": "./es/strategies/*.js",
      "require": "./lib/strategies/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    },
    "./vue": {
      "types": "./es/vue/index.d.ts",
      "import": "./es/vue/index.js",
      "require": "./lib/vue/index.cjs"
    },
    "./vue/*": {
      "types": "./es/vue/*.d.ts",
      "import": "./es/vue/*.js",
      "require": "./lib/vue/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,umd",
    "build:clean": "ldesign-builder build -f esm,cjs,umd --clean",
    "build:watch": "ldesign-builder build --watch",
    "dev": "rollup -c -w",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "lint:ci": "eslint . --config .eslintrc.ci.json",
    "test": "vitest -c vitest.local.config.ts",
    "test:ui": "vitest -c vitest.local.config.ts --ui",
    "test:run": "node scripts/run-vitest.mjs -c vitest.local.config.ts --reporter=basic --silent",
    "test:coverage": "node scripts/run-vitest.mjs -c vitest.local.config.ts --coverage",
    "test:ci": "node scripts/run-vitest.mjs --config vitest.config.ci.ts",
    "test:performance": "node scripts/performance-test.js",
    "test:benchmark": "node scripts/benchmark.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "clean": "rimraf esm cjs dist types lib coverage .nyc_output",
    "size-check": "size-limit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "ci": "pnpm lint:ci && pnpm type-check && pnpm test:ci && pnpm build && pnpm docs:build && pnpm docs:check-links",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "release": "semantic-release",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run",
    "deploy": "tsx ../../tools/deploy/package-deployer.ts cache",
    "deploy:beta": "tsx ../../tools/deploy/package-deployer.ts cache --tag beta",
    "deploy:alpha": "tsx ../../tools/deploy/package-deployer.ts cache --tag alpha",
    "deploy:dry-run": "tsx ../../tools/deploy/package-deployer.ts cache --dry-run",
    "build:dev": "node scripts/build.js --dev",
    "docs:check-links": "node scripts/docs-check-links.js",
    "test:fast": "node scripts/run-vitest.mjs --fast -c vitest.local.config.ts"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@ldesign/builder": "workspace:*",
    "@playwright/test": "^1.40.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.0.0",
    "glob": "^10.3.10",
    "jsdom": "^24.0.0",
    "playwright": "^1.40.1",
    "rimraf": "^5.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.12",
    "vitepress": "^1.0.0",
    "vitest": "^2.0.0",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  },
  "size-limit": [
    {
      "path": "src/index.ts",
      "limit": "50 KB",
      "environment": "node"
    }
  ],
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  }
}