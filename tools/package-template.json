{
  "name": "@ldesign/[PACKAGE_NAME]",
  "version": "0.1.0",
  "description": "[PACKAGE_DESCRIPTION]",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/[PACKAGE_NAME].umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/[PACKAGE_NAME].umd.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "size-check": "size-limit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "clean": "rimraf dist es lib types coverage"
  },
  "keywords": [
    "ldesign",
    "vue3",
    "typescript",
    "[PACKAGE_NAME]"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/[PACKAGE_NAME]"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/[PACKAGE_NAME]#readme",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@playwright/test": "^1.40.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.0.0",
    "happy-dom": "^13.3.1",
    "rimraf": "^5.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.12",
    "vitepress": "^1.0.0",
    "vitest": "^2.0.0",
    "vue": "^3.4.15",
    "vue-tsc": "^2.0.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/index.cjs",
      "limit": "50 KB"
    },
    {
      "path": "dist/[PACKAGE_NAME].umd.js",
      "limit": "60 KB"
    }
  ]
}